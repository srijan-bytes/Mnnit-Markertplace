<%- include("partials/headerB") -%>
<!-- boxes of other products -->
<section id="pricing" class="container-fluid">
  <h2>Others</h2>
  <h4>Looking for books? <a href="/books/<%=user%>" style="color:#11999e">Click Here!</a></h4>
  <div class="row">
      <%newItems.forEach(function(item){%>
    <div class="pricing-column col-lg-4 col-md-6">
      <div class="card ">
        <div class="card-header">
          <h3><%=item.pname%></h3>
        </div>
        <div class="card-body">
          <h2>Rs. <%=item.sellp%> </h2>
          <p>Seller Name: <%=item.name%></p>
          <button type="button" class="btn btn-outline-dark"><a href="/otherdB/<%=user%>/<%=item._id%>" class="know">Know More</a></button>
        </div>
      </div>
    </div>
    <%});%>
  </div>

  </section>
  <%- include("partials/footer") -%>>
