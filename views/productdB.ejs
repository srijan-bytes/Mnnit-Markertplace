  <div class="row" style="display:flex;">
    <div class="column" style="flex:50%;">
      <div class="image-block" style="margin: 20px;">
        <img src= <%=imageUrl%> alt="Product Image" class="w3-image" style="width:200%;max-width:800px">
      </div>
      
    </div>
    <div class="column" style="flex:50%;">
      <div class="container-fluid">
        <h2><%=pname%> Rs. <%=sellp%></h2>
        <p>
         
          Seller Name: <%=name%><br><br><br>
          <%=pname%> Details: <br />
          1. Brand: <%=brand%><br>
          2. Year: <%=year%><br>
          3. Condition: <%=condition%><br>
          4. Others: <%=others%><br><br>
          Seller Details:<br>
          1. Phone No: <%=phone%><br />
          2. EmailId: <%=email%><br>
          3. Hostel name : <%=hostel%><br>
          4. Room No: <%=room%><br>
          5. More: <%=time%><br />
        
          
        </p>
    
          <button type="button" class="btn btn-outline-dark"><a href="/products/<%=user%>" class="know" >Go Back</a></button>
      </div>
    </div>
  </div>
    
    
</div>

  
</section>
<%- include("partials/footer") -%>
